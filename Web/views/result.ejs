        <br><br><br><br><br><br><br>
        <!--Total Result count-->

        <div class="row">
            <div class="col-sm">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Total Request</h5>
                        <button type="button" class="btn btn-primary">
                            <%=countReqs %>
                        </button>
                        <p class="card-text" style="color:black;"> Total error code count</p>
                        <button type="button" class="btn btn-success"><b>2XX:</b>
                            <%=sum2xxs %>
                        </button>
                        <a class="btn btn-info"><b>3XX:</b>
                            <%=sum3xxs %>
                        </a>
                        <button type="button" class="btn btn-danger"><b>4XX:</b>
                            <%=main4xxs + sub4xxs %>
                        </button>
                        <a class="btn btn-warning"><b>5XX:</b>
                            <%=main5xxs + sub5xxs %>
                        </a>
                    </div>
                </div>
            </div>
        </div>

        <!--Error count-->

        <div class="row">
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Backend For Frontend (BFF)</h5>
                        <p class="card-text" style="color:black;">Error code count</p>
                        <button type="button" class="btn btn-danger"><b>4XX:</b>
                            <%=main4xxs %>
                        </button>
                        <a class="btn btn-warning"><b>5XX:</b>
                            <%=main5xxs %>
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Microservice API</h5>
                        <p class="card-text" style="color:black;">Error code count</p>
                        <button type="button" class="btn btn-danger"><b>4XX:</b>
                            <%=sub4xxs %>
                        </button>
                        <a class="btn btn-warning"><b>5XX:</b>
                            <%=sub5xxs %>
                        </a>
                    </div>
                </div>
            </div>
        </div>

        <br>
        <!--Both Exception list-->
        <div id="accordion4">
            <div class="card">
                <div class="card-header" id="headingFour">
                    <h4 class="mb-0">
                        <span>
                            <button class="btn btn-link" data-toggle="collapse" data-target="#collapseFour"
                                aria-expanded="true" aria-controls="collapseFour">
                                <h5>Both BFF and Microservice API that contain error exception</h5>
                            </button>
                        </span>
                        <span class="count">
                            <%= bothLeakIds.length %>
                        </span>
                    </h4>
                </div>

                <div id="collapseFour" class="collapse" aria-labelledby="headingFour" data-parent="#accordion4">
                    <div class="card-body">
                        <ul class="list-group">

                            <% for(let result of results){ %>
                                <% for(let trackId of bothLeakIds){ %>
                                    <% if(result.request.subrequest==trackId.id){ %>
                                        <li class="list-group-item list-group-item-action list-group-item-danger">
                                            <div class="main">
                                                <h5>
                                                    <span class="name">Request: <%= result.request.method %>
                                                            <%= result.request.uri %></span>
                                                    <span class="status">
                                                        <%= result.request.status_code %>
                                                    </span>
                                                </h5>
                                            </div>
                                        </li>

                                        <% for(let subrequest of result.subrequest){ %>
                                            <li class="list-group-item">
                                                <div class="sub">
                                                    <span class="name">Subrequest: <%= subrequest.method %>
                                                            <%= subrequest.uri %></span>
                                                    <span class="status">
                                                        <%= subrequest.status_code %>
                                                    </span>
                                                </div>
                                            </li>

                                            <% } %>
                                                <br>
                                                <% } %>
                                                    <% } %>
                                                        <% } %>

                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <br>
        <!--BFF Exception list-->
