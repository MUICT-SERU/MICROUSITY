                                            </div>
                                        </li>

                                        <% for(let subrequest of result.subrequest){ %>
                                            <li class="list-group-item">
                                                <div class="sub">
                                                    <span class="name">Subrequest: <%= subrequest.method %>
                                                            <%= subrequest.uri %></span>
                                                    <span class="status">
                                                        <%= subrequest.status_code %>
                                                    </span>
                                                </div>
                                            </li>

                                            <% } %>
                                                <br>
                                                <% } %>
                                                    <% } %>
                                                        <% } %>

                        </ul>
                    </div>
                </div>
            </div>

        </div>




    </div>

    <script>



        //function that tigger when clicking the submit button
        btnDictSubmit.on('click', function () {
            let dict = inputDict.val()
            let dyn = inputDyn.val()
            submit(dict, dyn)
        })

        function submit(dict, dyn) {

            console.log(dict)
            var parseDict = JSON.parse(dict);
            console.log(parseDict)
            $.ajax
                ({
                    type: "GET",
                    dataType: 'json',
                    async: false,
                    url: 'http://localhost:8080/saveDict_json',
                    data: { data: JSON.stringify(parseDict) },
                    success: function () { alert("Success"); },
                    failure: function () { alert("Error"); }
                });

            console.log(dyn)
            var parseDyn = JSON.parse(dyn);
            console.log(parseDyn)
            $.ajax
                ({
                    type: "GET",
                    dataType: 'json',
                    async: false,
                    url: 'http://localhost:8080/saveDyn_json',
                    data: { data: JSON.stringify(parseDyn) },
                    success: function () { alert("Success"); },
                    failure: function () { alert("Error"); }
                });

        }

    </script>

    </div>
</body>

